{
  "name": "poacher",
  "private": true,
  "devDependencies": {
    "@prettier/plugin-ruby": "^4.0.4",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.25.8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.17"
  },
  "description": "A Rails app with Node.js, Tailwind, Chartkick for the cheese industry",
  "engines": {
    "node": "23.7.0"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
    "start": "esbuild app/javascript/* --bundle --watch",
    "build:css": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css --minify"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.5",
    "apexcharts": "^3.51.0",
    "chart.js": "^4.4.3",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-adapter-dayjs": "^1.0.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "chartkick": "^5.0.1",
    "dayjs": "^1.11.13",
    "flatpickr": "^4.6.13",
    "flowbite": "^2.4.1",
    "highcharts": "^12.1.2",
    "html5-qrcode": "^2.3.8",
    "postcss": "^8.4.40",
    "primereact": "^10.8.4",
    "puppeteer": "^24.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "signature_pad": "^5.0.4",
    "stimulus": "^3.2.2"
  },
  "version": "1.22.19",
  "main": "tailwind.config.js",
  "directories": {
    "lib": "lib"
  },
