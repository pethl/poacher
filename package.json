  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.5",
    "apexcharts": "^3.51.0",
    "chart.js": "^4.4.3",
    "chartkick": "^5.0.1",
    "flowbite": "^2.4.1",
    "postcss": "^8.4.40",
    "primereact": "^10.8.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "signature_pad": "^5.0.4"
  },
  "version": "1.22.19"
}
