{
  "name": "poacher",
  "private": true,
  "devDependencies": {
    "@prettier/plugin-ruby": "^4.0.4",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.23.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.17"
  },
  "description": "A Rails app with Node.js, Tailwind, Chartkick for the cheese industry",
  "engines": {
    "node": "22.13.1"
  },
  "scripts": {
