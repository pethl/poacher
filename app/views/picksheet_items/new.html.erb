<main class="container">
  <%= link_to sanitize("&larr; Back to Pick Sheet"), picksheet_path(@picksheet) %>

  <div class="header">
    <h1>New Line Item</h1>
  </div>

  <%= turbo_frame_tag dom_id(PicksheetItem.new) do %>
    <%= render "form_new", picksheet: @picksheet, picksheet_item: @picksheet_item %>
  <% end %>
</main>

